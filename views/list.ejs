
<%- include('header') %> 


    <div class="box" id="heading">
      <h1> <%=listOfTitle%></h1>
    </div>

    <div class="box">
      <% for( let index = 0; index < newListItem.length; index++ ) { %>
      <div class="item">
        <input type="checkbox" />
        <p><%= newListItem[index] %></p>
      </div>
      <% } %>

      <form class="item" action="/" method="post">
        <input
          type="text"
          name="name"
          placeholder="enter your new item"
          autocomplete="off"
        />

        <button type="submit" name="save" value=<%=listOfTitle%> >+</button>
      </form>
    </div>

    <%- include("footer") %> 